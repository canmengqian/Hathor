<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zzz.hathor.codemaker.mapper.sqlite.TableInfoMapper">
    <select id="queryTables" resultType="com.zzz.hathor.codemaker.domain.entity.TableInfoEntity">
        SELECT name as tableName,sql FROM sqlite_master WHERE type='table' ORDER BY name;
    </select>
</mapper>